{
  "name": "mango",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "cross-env _ENV=local bun run --watch ./index.ts",
    "test": "bun run --watch ./test/custom.test.ts",
    "build:linux": "rm -rf ./dist/server && bun build --compile --target=bun-linux-x64 --minify --sourcemap --bytecode ./index.ts --outfile dist/server",
    "db:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "db:introspect": "prisma introspect",
    "db:studio": "prisma studio"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/figlet": "^1.7.0",
    "cross-env": "^7.0.3",
    "prisma": "^6.2.1"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/cron": "^1.2.0",
    "@elysiajs/swagger": "^1.2.0",
    "@mango/core": "workspace: *",
    "@mango/extends": "workspace: *",
    "@mango/types": "workspace: *",
    "@mango/utils": "workspace: *",
    "@prisma/client": "^6.1.0",
    "elysia": "1.1.27",
    "elysia-decorators": "^1.0.2",
    "glob": "^11.0.1",
    "logestic": "^1.2.4",
    "reflect-metadata": "^0.2.2"
  }
}
